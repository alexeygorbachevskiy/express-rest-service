{
  "name": "express-rest-service",
  "version": "0.0.1",
  "description": "REST service starter for RS School",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "start": "nodemon -L --exitcrash",
    "prod": "ts-node build/main.js",
    "lint": "eslint --ext .js,.ts --fix src",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "test:load": "artillery run -o ./artillery-report.json ./artillery.yaml",
    "artillery:report": "artillery report ./artillery-report.json",
    "migration:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts migration:generate -n Migration",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts migration:run"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter",
    "nest",
    "express"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fastify-swagger": "^4.8.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "stream": "^0.0.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^14.14.36",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.25.0",
    "artillery": "^1.7.6",
    "chai": "^4.3.4",
    "dirty-chai": "^2.0.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "10.5.4",
    "nodemon": "^2.0.9",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  }
}
